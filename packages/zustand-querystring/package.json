{
  "name": "zustand-querystring",
  "version": "0.4.1",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "zustand",
    "querystring"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/nitedani/zustand-querystring"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./format/compact": {
      "types": "./dist/format/compact.d.ts",
      "import": "./dist/format/compact.mjs",
      "require": "./dist/format/compact.js"
    },
    "./format/plain": {
      "types": "./dist/format/plain.d.ts",
      "import": "./dist/format/plain.mjs",
      "require": "./dist/format/plain.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "format/compact": [
        "./dist/format/compact.d.ts"
      ],
      "format/plain": [
        "./dist/format/plain.d.ts"
      ]
    }
  },
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ],
    "entry": {
      "index": "src/index.ts",
      "format/compact": "src/format/compact.ts",
      "format/plain": "src/format/plain.ts"
    },
    "dts": true
  },
  "files": [
    "/dist"
  ],
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser-playwright": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^4.0.12",
    "vitest-browser-react": "^2.0.2",
    "zustand": "^5.0.3"
  }
}
